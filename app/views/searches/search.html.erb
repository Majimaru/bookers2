<div class="container">
  <div class="row">
    <div class="col-12">
      <% if @model == "User" %>
        <h3>Users search for "<%= @content %>"</h3>
        <%= render "users/index", users: @records, hidden: "true" %>
      <% else %>
        <h3>Books search for "<%= @content %>"</h3>
          <%= render "books/index", books: @records, hidden: "true" %>
      <% end %>
    </div>
  </div>
</div>